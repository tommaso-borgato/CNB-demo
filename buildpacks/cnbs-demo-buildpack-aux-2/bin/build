#!/usr/bin/env bash

set -euo pipefail

LAYERS_DIR="$1"
env_dir="$2/env"
plan_path="$3"

echo "PIPPO-3 ${PWD} - $LAYERS_DIR" >> PIPPO.txt
cat PIPPO.txt

echo "=================================================="
echo "$LAYERS_DIR"
echo "=================================================="
pwd
ls -l

exit 0